<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="leavedays" namespace="leavedays.Models">
  <class name="Module" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="ModuleId" type="int">
      <generator class="native" />
    </id>

    <property name="DefaultModuleId" />
    <property name="Price" />
    <property name="IsActive" />
    <property name="IsLocked" />
    <property name="LicenseId" />
  </class>

</hibernate-mapping>