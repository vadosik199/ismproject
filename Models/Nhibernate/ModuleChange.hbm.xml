<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="leavedays" namespace="leavedays.Models">
  <class name="ModuleChange" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="ModuleId" />
    <property name="IsLocked" />
    <property name="Price" />
    <property name="StartDate"/>
  </class>

</hibernate-mapping>